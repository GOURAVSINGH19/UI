{
  "extends": "@workspace/typescript-config/nextjs.json",
  "compilerOptions": {
    "ignoreDeprecations":"6.0",
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    "next.config.js",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "paths": {
    "@/*": [
      "./*"
    ]
  }
}